{
  "version": 2,
  "name": "blueprint-generator-pro",
  "builds": [
    {
      "src": "web_app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/web_app/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web_app/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "web_app/main.py": {
      "runtime": "python3.9"
    }
  }
}
